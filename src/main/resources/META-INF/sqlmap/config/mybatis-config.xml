<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- 김훈 -->
		<typeAlias type="com.lec.jeju.vo.MapSpot" alias="MapSpot"/>
		<typeAlias type="com.lec.jeju.vo.MapRestaurant" alias="MapRestaurant"/>
		<typeAlias type="com.lec.jeju.vo.MapHotel" alias="MapHotel"/>
		
		<!-- 강성빈 -->
		<typeAlias type="com.lec.jeju.vo.FestivalBoard" alias="Festival" />
