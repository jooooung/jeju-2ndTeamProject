<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- 김훈 -->
		<typeAlias type="com.lec.jeju.vo.MapSpot" alias="MapSpot"/>
		<typeAlias type="com.lec.jeju.vo.MapRestaurant" alias="MapRestaurant"/>
		<typeAlias type="com.lec.jeju.vo.MapHotel" alias="MapHotel"/>
		
		<!-- 강성빈 -->
		<typeAlias type="com.lec.jeju.vo.FestivalBoard" alias="Festival" />
		
		
		<!-- 안중권 -->
		<typeAlias type="com.lec.jeju.vo.Hotel" alias="Hotel"/>
		<typeAlias type="com.lec.jeju.vo.Hreservation" alias="Hreservation"/>
		<typeAlias type="com.lec.jeju.vo.HotelComment" alias="HotelComment"/>
	</typeAliases>
	<mappers>
		<!-- 김훈 -->
		<mapper resource="META-INF/sqlmap/mappers/MapSpot.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/MapRestaurant.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/MapHotel.xml"/>
		
		<!-- 강성빈 -->
		<mapper resource="META-INF/sqlmap/mappers/FestivalBoard.xml" />
		
		<!-- 안중권 -->
		<mapper resource="META-INF/sqlmap/mappers/hotel.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/hReservation.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/hotelComment.xml"/>
	</mappers>
</configuration>