		<mapper resource="META-INF/sqlmap/mappers/MapSpot.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/MapRestaurant.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/MapHotel.xml"/>
		
		<!-- 강성빈 -->
		<mapper resource="META-INF/sqlmap/mappers/FestivalBoard.xml" />
		<mapper resource="META-INF/sqlmap/mappers/Member.xml" />
